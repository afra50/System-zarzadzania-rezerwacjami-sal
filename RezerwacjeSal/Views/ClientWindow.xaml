<Window x:Class="RezerwacjeSal.Views.ClientWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Panel Klienta" Height="400" Width="600">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Text="Panel Klienta" FontSize="24" Margin="0,0,0,20"/>
            <Button Content="Rezerwuj salę" Width="200" Height="40" Margin="0,10" Click="ReserveRoom_Click"/>
            <Button Content="Moje rezerwacje" Width="200" Height="40" Margin="0,10"/>
            <Button Content="Wyloguj" Width="200" Height="40" Click="Logout_Click" Margin="0,10"/>
        </StackPanel>
    </Grid>
</Window>

